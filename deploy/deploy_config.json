{
    "name": "freeskidb",
    "type": "Microsoft.ContainerInstance/containerGroups",
    "apiVersion": "2018-06-01",
    "location": "northeurope",
    "properties": {
      "containers": [
        {
          "name": "mycontainer",
          "properties": {
            "image": "mmalvik/freeskidb",
            "ports": [
              {
                "protocol": "tcp",
                "port": "80"
              }
            ],
            "environmentVariables": [
              {
                "name": "CosmosUri",
                "secureValue": "value"
              },
              {
                "name": "CosmosKey",
                "secureValue": "value"
              },
              {
                "name": "AzureSearchUri",
                "secureValue": "value"
              },
              {
                "name": "AzureSearchKey",
                "secureValue": "value"
              },
              {
                "name": "AzureSearchServiceName",
                "value": "value"
              }
            ],
            "resources": {
              "requests": {
                "memoryInGB": "1",
                "cpu": "1"
              }
            }
          }
        }
      ],
      "restartPolicy": "always",
      "ipAddress": {
        "ports": [
          {
            "protocol": "tcp",
            "port": "80"
          }
        ],
        "type": "Public",
        "ip": "string",
        "dnsNameLabel": "freeskidb-container"
      },
      "osType": "Linux"
    }
  }